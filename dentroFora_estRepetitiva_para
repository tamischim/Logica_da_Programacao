Algoritmo "problema_dentroFora"

Var

   n, x, i, dentro, fora : inteiro
   
Inicio

      dentro <- 0
      fora <- 0

      escreval ("BRASIL ESCOLA")
      escreval ("Números entre 10 e 20")
      escreval
      escreva  ("Quantos números você irá digitar? ")
      leia     (n)
      
      para i de 1 ate n faca
      
        escreva ("Digite um número: ")
        leia (x)
        
        se (x >= 10) e (x <= 20) entao
           dentro <- dentro + 1
        senao
           fora <- fora + 1
        fimse
        
      fimpara
      
      escreval (dentro, " Dentro")
      escreval (fora, " Fora")

Fimalgoritmo
